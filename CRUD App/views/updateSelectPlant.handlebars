<div class="container">
    <div class="row justify-content-center">
        <h1>Update Plants</h1>
    </div>
</div>
<form method='GET' id="updateformid">
    <div class="form-group">
        <label for="plant">Select Plant to Update</label>
        <select class="form-control" name="plants" id="plants">
            {{#each plants}}
            <option>{{this}}</option>
            {{/each}}
        </select>
    </div>
    <button id="updateplantsubmitid">Submit</button>
</form>